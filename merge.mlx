<!DOCTYPE FilterScript>
<FilterScript>
 <filter name="Select non Manifold Edges "/>
 <filter name="Merge Close Vertices">
  <Param tooltip="All the vertices that closer than this threshold are merged together. Use very small values, default values is 1/10000 of bounding box diagonal. " value="0.088728" min="0" max="8.87283" type="RichAbsPerc" description="Merging distance" name="Threshold"/>
 </filter>
</FilterScript>
